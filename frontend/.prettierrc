{
    "semi": true,
    "trailingComma": "es5",
    "useTabs": true,
    "tabWidth": 4,
    "endOfLine": "auto",
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "jsxSingleQuote": true,
    "printWidth": 80,
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "singleAttributePerLine": true
}